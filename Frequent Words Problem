#include <iostream>

using namespace std;


/*
* Frequent Words Problem: Find the most frequent k-mers in a string.
* params: A string Text and an integer k.
* return:All most frequent k-mers in Text.
*example:   
	input:
		ACGTTGCATGTCGCATGATGCATGAGAGCT
    	4
    output:
    	CATG GCAT
*/

void frequentWord(string a,int k)
{
	int kmers = a.length()-k+1;
	string parted[kmers];
	for(int i=0;i<=a.length()-k;i++)
	{
		parted[i]=a.substr(i,k);
	}
	int current_count = 0;
	for(int i=0;i<kmers-1;i++)
	{
		current_count = 1;
		if(parted[i] != "-1")
		{
			for(int a=i+1;a<kmers;a++)
			{
				if(parted[i] == parted[a])
				{
					current_count++;
					parted[a] = "-1";
				}
			}
		}
		if(current_count>4)
			cout << parted[i] << " " << current_count << endl;
	}



}


int main()
{
	string a;
	cin>>a;
	int k;
	cin >>k;
	frequentWord(a,k);
}

